{
    "name": "lab5",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@auth0/auth0-spa-js": "^1.19.3",
        "@testing-library/jest-dom": "^5.16.1",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^13.5.0",
        "eslint": "^7.11.0",
        "eslint-plugin-jest": "^25.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "prettier": "^2.4.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "5.0.0",
        "react-transition-group": "^4.4.2"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "lint": "eslint \"**/*.js\" --ignore-path .gitignore",
        "lint:fix": "eslint \"**/*.js\" --fix --ignore-path .gitignore",
        "format": "prettier --check . \"!**/*.css\" --ignore-path .gitignore",
        "format:fix": "prettier --write . \"!**/*.css\" --ignore-path .gitignore",
        "stylelint": "stylelint \"**/*.css\" --ignore-path .gitignore",
        "stylelint:fix": "stylelint \"**/*.css\" --fix --ignore-path .gitignore",
        "prepare": "husky install",
        "eject": "react-scripts eject"
    },
    "devDependencies": {
        "husky": "^7.0.4",
        "lint-staged": "^11.2.6",
        "stylelint": "^14.0.1",
        "stylelint-config-standard": "^23.0.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
